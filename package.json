    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "fastify-multer": "^2.0.3",
    "joi": "^17.11.0",
    "json-api-serializer": "^2.6.6",
    "luxon": "^3.4.4",
    "nestjs-cls": "^3.6.0",
    "opossum": "^8.1.3",
