    "@nestjs/testing": "^10.2.10",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.93",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.6",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/jest": "^29.5.7",
    "@types/json-api-serializer": "^2.6.6",
    "@types/luxon": "^3.3.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/opossum": "^8.1.6",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/sequelize": "^4.28.19",
